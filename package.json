{
  "name": "tester",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "jira-prepare-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurastich/tester.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurastich/tester/issues"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J]\n$M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": false,
    "allowEmptyCommitMessage": false
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  },
  "homepage": "https://github.com/yurastich/tester#readme",
  "dependencies": {
    "jira-prepare-commit-msg": "^1.5.2",
    "shared-git-hooks": "^1.2.1"
  },
  "devDependencies": {
    "husky": "^6.0.0"
  }
}
